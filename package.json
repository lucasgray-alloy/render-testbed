{
  "name": "alloy-api",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "author": "Lucas Gray <lucas@myalloy.com>",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "run-s build-and:test",
    "tdd": "run-p watch:*",
    "commitmsg": "validate-commit-msg",
    "precommit": "run-s lint:* build",
    "prepush": "run-s build-and:test",
    "lint:ts": "tslint --project tsconfig.json",
    "lint-and-fix:-ts": "tslint --project tsconfig.json --fix",
    "watch:ts": "tsc -w",
    "watch:test": "mocha dist/**/*.test.js -w --colors --inspect --growl",
    "test-once": "mocha dist/**/*.test.js --inspect --colors"
  },
  "dependencies": {
    "express": "^4.17.3",
    "typescript": "3.6.4"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/express": "^4.17.13",
    "@types/mocha": "5.2.5",
    "@types/node": "^12.12.5",
    "all-contributors-cli": "^6.4.0",
    "chai": "4.1.2",
    "husky": "0.14.3",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "prettier": "^2.5.1",
    "sinon": "6.1.5",
    "tslint": "5.11.0",
    "tslint-config-acamica": "2.0.0",
    "tslint-config-prettier": "^1.18.0",
    "validate-commit-msg": "2.14.0"
  }
}
